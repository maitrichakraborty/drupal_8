{#
/**
 * @file meeting_entity.html.twig
 * Default theme implementation to present Meeting data.
 *
 * This template is used when viewing Meeting pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_meeting_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('meeting_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
